
{% if blocks|length %}

    {% for block in blocks.all() %}
        {% switch block.type %}


        {# Article Body #}
        {% case "articleBody" %}
            {% include '_components/content-blocks/article' %}
        
        {# Button #}
        {% case "button" %}
            {% include '_components/content-blocks/button' %}
        
        {# Link Asset List #}
        {% case "linkAssetList" %}
            {% include '_components/content-blocks/link-asset-list' %}
        
        {# Contact Card #}
        {% case "contactCard" %}
            {% include '_components/content-blocks/article' %}
        
        {# Text List 2 Col #}
        {% case "textList" %}
            {% include '_components/content-blocks/text-list' %}
        
        {# Billboard #}
        {% case "billboard" %}
            {% include '_components/content-blocks/billboard' %}

        {# Personel List #}
        {% case "personelList" %}
            {% include '_components/content-blocks/personel-list' %}

        {# Fullwidth Image #}
        {% case "fullwidthImage" %}
            {% include '_components/content-blocks/fullwidth-image' %}

        
        {% default %}
            This block isn't defined!
        
        {% endswitch %}
    {% endfor %}
    

{% endif %}